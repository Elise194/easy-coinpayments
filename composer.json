{
    "name": "elise194/easy-coinpayments",
    "description": "Coinpayments API package for Laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "coinpayments",
        "api"
    ],
    "authors": [
        {
            "name": "Churakov Mike"
        },
        {
            "name": "Churakova Elizaveta"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/database": "^5.5|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.3",
        "orchestra/testbench": ">=3.0",
        "phpunit/php-code-coverage": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Elise194\\EasyCoinPayments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elise194\\EasyCoinPayments\\CoinpaymentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "run:unit": [
            "phpunit ./tests"
        ]
    }
}
